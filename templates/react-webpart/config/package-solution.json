{
    "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/package-solution.schema.json",
    "solution": {
        "name": "{{SOLUTION_NAME}}-client-side-solution",
        "id": "{{SOLUTION_ID}}",
        "version": "1.0.0.0",
        "includeClientSideAssets": true,
        "skipFeatureDeployment": true,
        "isDomainIsolated": false,
        "developer": {
          "name": "",
          "websiteUrl": "",
          "privacyUrl": "",
          "termsOfUseUrl": "",
          "mpnId": "Undefined-1.21.1"
        },
        "metadata": {
          "shortDescription": {
            "default": "{{SOLUTION_NAME}} description"
          },
          "longDescription": {
            "default": "{{SOLUTION_NAME}} description"
          },
          "screenshotPaths": [],
          "videoUrl": "",
          "categories": []
        },
        "features": [
            {
                "title": "{{SOLUTION_NAME}} Feature",
                "description": "The feature that activates elements of the {{SOLUTION_NAME}} solution.",
                "id": "{{FEATURE_ID}}",
                "version": "1.0.0.0"
            }
        ]
    },
    "paths": {
        "zippedPackage": "solution/{{SOLUTION_NAME}}.sppkg"
    }
